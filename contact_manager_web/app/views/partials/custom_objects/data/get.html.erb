partials/customObjects/data/get.html


<div class='row'>
	<form novalidate class="form-horizontal" name="form" >
    <div class="form-group" ng-repeat='key in customObjectDataKeys'>
      <label for="name" class="col-sm-2 control-label">{{key.name}}</label>
      <div class="col-sm-10">
      	<input class="form-control" ng-model="customObjectData[key.fieldName]" type='{{FieldType(key)}}'>
      </div>
    </div>
    <hr />
  	<div class="form-group">
  		<div class="col-sm-offset-2 col-sm-10">
  			<button type="button" class="btn btn-default" ng-disabled='!form.$valid' ng-click="Update()"><span ng-class="form.$dirty ? 'glyphicon glyphicon-floppy-disk' : 'glyphicon glyphicon-floppy-saved'"></span></button>
  		</div>
  	</div>
  </form>
</div>

<button type="button" ng-click="Delete()">Delete Data</button>
<br/><br/><br/>
